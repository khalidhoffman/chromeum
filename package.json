{
  "name": "chromeum",
  "version": "0.0.0",
  "description": "custom background page for chrome/chromium",
  "main": "app.js",
  "scripts": {
    "test": "mocha",
    "build-html": "pug index.pug --pretty; pug app/views/",
    "build-js": "webpack --config ./webpack.config.js --display-reasons",
